{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "modules": true,
    "quotes": [2, "single"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "comma-dangle": [0],
    "curly": [0],
    "strict": [0],
    "no-use-before-define": "nofunc"
  }
}
