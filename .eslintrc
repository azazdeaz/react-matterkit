{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "babel"
  ],
  "ecmaFeatures": {
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "space-infix-ops": [0],
    "quotes": [2, "single"],
    "semi": [2, "never"],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "comma-dangle": [0],
    "curly": [0],
    "strict": [0],
    "no-use-before-define": [2, "nofunc"],
    "babel/new-cap": [1],
    "new-cap": [0]
  }
}
