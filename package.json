{
  "name": "react-matterkit",
  "version": "0.0.0",
  "description": "React GUI component kit based on matterkit.io design and focusing on input fields.",
  "main": "./lib/index.js",
  "dependencies": {
    "lodash": "^3.6.0",
    "lodash.merge": "^3.0.3",
    "lodash.has": "^3.0.0",
    "react": "^0.13.1",
    "radium": "^0.10.1",
    "fast-levenshtein": "^1.0.6"
  },
  "peerDependencies": {
    "react": ">=0.12.2 <0.14.0"
  },
  "devDependencies": {
    "babelify": "^6.0.2",
    "browser-sync": "^2.5.3",
    "browserify": "^9.0.8",
    "exorcist": "^0.1.6",
    "gulp": "^3.8.11",
    "gulp-babel": "^5.1.0",
    "gulp-clean": "^0.3.1",
    "gulp-sourcemaps": "^1.5.1",
    "gulp-util": "^3.0.4",
    "lodash": "^3.6.0",
    "marked": "^0.3.3",
    "react-router": "^0.13.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.1.0"
  },
  "scripts": {
    "dev": "babel src --out-dir lib --watch",
    "prebuild": "rm -rf lib && rm -rf browser && mkdir browser",
    "build:lib": "babel src --out-dir lib",
    "build:browser": "browserify lib/index.js --standalone matterkit --debug | exorcist browser/matterkit.js.map > browser/matterkit.js",
    "build": "npm run build:lib && npm run build:browser",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/azazdeaz/react-matterkit.git"
  },
  "keywords": [
    "react",
    "matterkit",
    "gui",
    "ui",
    "input"
  ],
  "author": "Polgár András",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/azazdeaz/react-matterkit/issues"
  },
  "homepage": "https://github.com/azazdeaz/react-matterkit"
}
